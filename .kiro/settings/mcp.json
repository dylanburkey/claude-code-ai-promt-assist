{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": []
    }
  },
  "powers": {
    "mcpServers": {
      "power-supabase-local-supabase": {
        "url": "http://127.0.0.1:54321/mcp",
        "disabled": false
      }
    }
  },
  "cloudflare": {
    "command": "npx",
    "args": [
      "mcp-remote",
      "https://bindings.mcp.cloudflare.com/mcp",
      [
        "mcp-remote",
        "https://docs.mcp.cloudflare.com/mcp"
      ]
    ]
  }
}